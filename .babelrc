{
  "presets": [
    "env",
    "react"
  ],
  "plugins": [
    "lodash",
    "transform-object-rest-spread",
    [
      "module-resolver", {
        "root": [ "./src" ],
        "alias": {
          "app": "./src",
          "assets": "./src/assets",
          "components": "./src/components",
          "config": "./src/config",
          "pages": "./src/pages",
          "sections": "./src/sections",
          "store": "./src/store",
          "actions": "./src/store/actions"
        }
      }
    ]
  ]
}
